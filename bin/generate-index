#!/usr/bin/env node

var fs = require('fs')

var pngs = fs.readdirSync('png').reverse(),
    i = pngs.length

process.stdout.write("... | ... | ... |... \n")
process.stdout.write(":---: | :---: | :---: | :---: \n")

while(i--) {
  var png = pngs[i]

  if(!(/\.png$/i).test(png))
    continue

  var name = png.replace('.shp.png', '').replace('.png', '').replace(/-/g, ' ')

  if(i % 4 == 0) {
    process.stdout.write("![](https://raw.githubusercontent.com/Caged/portland-atlas/master/png/" + png + ") **" + name + "**  \n")
  } else {
    process.stdout.write("![](https://raw.githubusercontent.com/Caged/portland-atlas/master/png/" + png + ") **" + name + "** | ")
  }
}
